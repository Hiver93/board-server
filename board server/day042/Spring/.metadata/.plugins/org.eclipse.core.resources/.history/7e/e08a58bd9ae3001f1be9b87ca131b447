package com.example.demo.dto;

public class PostReqDto {

	public static class Create{
		private String title;
		private String content;
		public Create() {
			super();
		}
		void setTitle(String title) {
			this.title = title;
		}
		void setContent(String content) {
			this.content = content;
		}
		
		
	}
}
