package com.example.demo.dto;

public class PostResDto {

	public static record Detail(
			Integer id,
			String title,
			String content,
			Integer view,
			Integer userId,
			Integer comments,
			Integer likes,
			String nickname,
			String createdAt,
			String updatedAt
			) {
		
		private static record CommentItem(
				Integer id,
				String content,
				Integer userId,
				String nickname,
				String createdAt
				){
		}
	}
}
